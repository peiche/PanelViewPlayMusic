{
	"name": "Panel View for Play Music",
	"description": "Panelized extension for Play Music",
	"version": "2.0",
	"background": {
		"scripts": [
			"js/background.js"
		]
	},
	"browser_action": {
		"default_icon": "img/icon_128_gray.png"
	},
	"icons": {
		"128": "img/icon_128.png"
	},
	"permissions": [
		"tabs",
		"*://music.google.com/*",
		"*://play.google.com/music/*"
	],
	"content_scripts": [
		{
			"js": [
				"js/jquery-2.0.3.min.js"
			],
			"matches": [
				"*://music.google.com/*",
				"*://play.google.com/music/*"
			],
			"run_at": "document_start"
		},
		{
			"css": [
				"css/style-music.css"
			],
			"js": [
				"js/script-music.js"
			],
			"matches": [
				"*://music.google.com/*",
				"*://play.google.com/music/*"
			],
			"run_at": "document_end"
		}
	],
	"manifest_version": 2
}
